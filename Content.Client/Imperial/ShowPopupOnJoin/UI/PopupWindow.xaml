<DefaultWindow xmlns="https://spacestation14.io">
    <BoxContainer Orientation="Vertical">
        <BoxContainer>
            <RichTextLabel Name="TextContent"/>
        </BoxContainer>
        <TextureRect HorizontalAlignment="Center"  Name="QRImage"/>
        <BoxContainer Orientation="Horizontal">
            <Button Name="NeverShowButton" Text="{Loc ui-never-show}" HorizontalAlignment="Left"/>
            <Button Name="OpenLinkButton" Text="{Loc ui-open-in-browser}" HorizontalAlignment="Right"/>
        </BoxContainer>
    </BoxContainer>
</DefaultWindow>
